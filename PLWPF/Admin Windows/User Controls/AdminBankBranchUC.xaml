<UserControl x:Class="PLWPF.Admin_Windows.User_Controls.AdminBankBranchUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PLWPF.Admin_Windows.User_Controls"
             mc:Ignorable="d" 
             d:DesignHeight="60" d:DesignWidth="800">
    <Grid ShowGridLines="True">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="8*"/>
        </Grid.ColumnDefinitions>
        <Label Grid.Column="0" x:Name="BankNumber"/>
        <Label Grid.Column="1" x:Name="BankAccountNumber"/>
        <Label Grid.Column="2" x:Name="BranchNumber"/>
        <Label Grid.Column="3" x:Name="FullName"/>
    </Grid>
</UserControl>
